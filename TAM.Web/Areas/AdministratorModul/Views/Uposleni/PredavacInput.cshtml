
@{
    ViewData["Title"] = "Predavac";
    Layout = null;

    var titule = (List<SelectListItem>)TempData["titule"];
}

@model UposleniRegistracijaVM

<div class="form-group">
    <label asp-for="@Model.Titula"></label>
    <select asp-for="Titula" asp-items="titule" class="form-control"></select>
</div>
<div class="form-group">
    <label asp-for="@Model.CV"></label>
    <input asp-for="@Model.CV" type="file" />
</div>